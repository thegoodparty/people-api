{
  "name": "people-api",
  "version": "0.0.1",
  "description": "",
  "author": "GoodParty.org Team",
  "private": true,
  "license": "CC0-1.0",
  "scripts": {
    "build": "nest build",
    "copy": "ts-node ./copy.ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "nest start",
    "start:perf-local": "ENV_PATH=.env.perf-local NODE_ENV=perf-local nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "lint-format": "npm run lint && npm run format",
    "migrate:dev": "node ./scripts/disallow-perf-local.js && npx prisma migrate dev",
    "migrate:deploy": "node ./scripts/disallow-perf-local.js && npx prisma migrate deploy",
    "migrate:reset": "node ./scripts/disallow-perf-local.js && npx prisma migrate reset --force",
    "migrate:schema:dev": "node ./scripts/disallow-perf-local.js && tsx ./scripts/run-migrations.ts --mode dev --stage tables",
    "migrate:schema:create": "cd prisma/migrations-schema && npx prisma migrate dev --create-only",
    "migrate:indexes:dev": "node ./scripts/disallow-perf-local.js && tsx ./scripts/migrate-indexes.ts --mode dev --stage indexes",
    "migrate:indexes:create": "cd prisma/migrations-indexes && npx prisma migrate dev --create-only",
    "migrate:all:deploy": "npm run migrate:schema:deploy && npm run migrate:indexes:deploy",
    "db:pull": "node ./scripts/db-pull-after-migrate.js",
    "generate": "npx prisma generate",
    "seed": "npx prisma db seed",
    "perf:all": "sh -c '\n      k6 run --summary-trend-stats \"avg,med,p(50),p(95),p(99),min,max\" --env ONLY=small perf/people-get.ts || true;\n      k6 run --summary-trend-stats \"avg,med,p(50),p(95),p(99),min,max\" --env ONLY=large perf/people-get.ts || true;\n      k6 run --summary-trend-stats \"avg,med,p(50),p(95),p(99),min,max\" --env ONLY=small perf/stats-get.ts || true;\n      k6 run --summary-trend-stats \"avg,med,p(50),p(95),p(99),min,max\" --env ONLY=large perf/stats-get.ts || true;\n      k6 run --summary-trend-stats \"avg,med,p(50),p(95),p(99),min,max\" --env ONLY=small perf/sample-get.ts || true;\n      k6 run --summary-trend-stats \"avg,med,p(50),p(95),p(99),min,max\" --env ONLY=large perf/sample-get.ts || true\n    '",
    "perf:mixed": "k6 run --summary-trend-stats \"avg,med,p(50),p(95),p(99),min,max\" perf/mixed.ts"
  },
  "prisma": {
    "seed": "tsx ./seed/seed.ts",
    "schema": "prisma/generated-schema.prisma"
  },
  "dependencies": {
    "@fast-csv/format": "^5.0.5",
    "@fastify/cookie": "^11.0.2",
    "@fastify/cors": "^11.0.0",
    "@fastify/helmet": "^13.0.1",
    "@fastify/static": "^8.1.1",
    "@nestjs/common": "^11.0.11",
    "@nestjs/core": "^11.0.11",
    "@nestjs/platform-fastify": "^11.0.11",
    "@nestjs/swagger": "^11.0.6",
    "@prisma/client": "^6.12.0",
    "@types/yargs": "^17.0.35",
    "deepmerge-ts": "^7.1.5",
    "dotenv": "^16.4.7",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "module-alias": "^2.2.3",
    "nestjs-pino": "^4.4.1",
    "nestjs-zod": "^4.3.1",
    "newrelic": "^11.20.0",
    "pino-http": "^10.5.0",
    "prisma": "^6.12.0",
    "qs": "^6.13.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "yargs": "^18.0.0",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@faker-js/faker": "^9.9.0",
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.1",
    "@swc/cli": "^0.6.0",
    "@swc/core": "^1.10.7",
    "@types/express": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/lodash": "^4.17.16",
    "@types/node": "^22.13.9",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-unused-imports": "^4.1.4",
    "globals": "^16.0.0",
    "prettier": "^3.0.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.6",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.27.0"
  },
  "rootDir": "src"
}
