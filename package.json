{
  "name": "people-api",
  "version": "0.0.1",
  "description": "",
  "author": "GoodParty.org Team",
  "private": true,
  "license": "CC0-1.0",
  "scripts": {
    "build": "nest build",
    "test": "vitest run",
    "test:watch": "vitest",
    "copy": "ts-node ./copy.ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "nest start",
    "start:perf-local": "ENV_PATH=.env.perf-local NODE_ENV=perf-local nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node -r ./dist/src/otel.js dist/src/main.js",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "lint-format": "npm run lint && npm run format",
    "migrate:dev": "node ./scripts/disallow-perf-local.js && npx prisma migrate dev",
    "migrate:deploy": "node ./scripts/disallow-perf-local.js && npx prisma migrate deploy",
    "migrate:reset": "node ./scripts/disallow-perf-local.js && npx prisma migrate reset --force",
    "generate": "npx prisma generate",
    "seed": "npx prisma db seed",
    "perf:core": "bash scripts/perf-core.sh",
    "perf:mixed": "k6 run --summary-trend-stats \"avg,med,p(50),p(95),p(99),min,max\" perf/mixed.ts"
  },
  "prisma": {
    "seed": "tsx ./seed/seed.ts",
    "schema": "prisma/schema"
  },
  "dependencies": {
    "@fast-csv/format": "^5.0.5",
    "@fastify/cookie": "^11.0.2",
    "@fastify/cors": "^11.0.0",
    "@fastify/helmet": "^13.0.1",
    "@fastify/otel": "^0.16.0",
    "@fastify/static": "^8.1.1",
    "@nestjs/common": "^11.0.11",
    "@nestjs/core": "^11.0.11",
    "@nestjs/platform-fastify": "^11.0.11",
    "@nestjs/swagger": "^11.0.6",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/exporter-logs-otlp-http": "^0.212.0",
    "@opentelemetry/exporter-metrics-otlp-http": "^0.212.0",
    "@opentelemetry/exporter-trace-otlp-http": "^0.212.0",
    "@opentelemetry/host-metrics": "^0.38.3",
    "@opentelemetry/instrumentation-http": "^0.212.0",
    "@opentelemetry/instrumentation-pino": "^0.58.0",
    "@opentelemetry/resources": "^2.5.1",
    "@opentelemetry/sdk-logs": "^0.212.0",
    "@opentelemetry/sdk-metrics": "^2.5.1",
    "@opentelemetry/sdk-node": "^0.212.0",
    "@opentelemetry/semantic-conventions": "^1.40.0",
    "@prisma/client": "^6.5.0",
    "@prisma/instrumentation": "^7.4.2",
    "deepmerge-ts": "^7.1.5",
    "dotenv": "^16.4.7",
    "es-toolkit": "^1.44.0",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "module-alias": "^2.2.3",
    "murmurhash": "^2.0.1",
    "nestjs-pino": "^4.4.1",
    "nestjs-zod": "^4.3.1",
    "newrelic": "^11.20.0",
    "pino-http": "^10.5.0",
    "prisma": "^6.4.1",
    "prisma-json-types-generator": "^3.6.2",
    "qs": "^6.13.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@faker-js/faker": "^10.2.0",
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.1",
    "@swc/cli": "^0.6.0",
    "@swc/core": "^1.15.11",
    "@types/express": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/lodash": "^4.17.16",
    "@types/node": "^22.13.9",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-unused-imports": "^4.1.4",
    "globals": "^16.0.0",
    "pino-pretty": "^13.1.3",
    "prettier": "^3.0.0",
    "prisma": "^6.5.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.53.0",
    "unplugin-swc": "^1.5.9",
    "vite-tsconfig-paths": "^6.0.5",
    "vitest": "^4.0.18"
  },
  "rootDir": "src"
}
