model DistrictStats {
  districtId                     String   @id @map("district_id")
  updatedAt                      DateTime @map("updated_at")
  totalConstituents              Int      @map("total_constituents")
  totalConstituentsWithCellPhone Int      @map("total_constituents_with_cell_phone")

  /// [DistrictStatsBuckets]
  buckets Json @map("buckets")

  @@schema("green")
}
