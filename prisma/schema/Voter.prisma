model Voter {
    // Core Voter Information
    id                        String   @id @map("id") @db.Uuid
    createdAt                 DateTime @default(now()) @map("created_at")
    updatedAt                 DateTime @updatedAt @map("updated_at")
    LALVOTERID                String   @unique @db.Text() // Permanent and unique ID generated by L2 for a single voter
    State                     USState // Two-letter state code for unified table partitioning
    AbsenteeTypes_Description String?  @db.Text() // Military, Long Term, In State, Out of State, Permanent Out of Country, Mail Precinct, Special, Temporary, Permanent U.S., Federal
    Active                    String?  @db.Text() // Voters_Active
    Age                       String?  @db.Text() // Age is based on current year minus year of birth.
    Age_Int                   Int?     @db.Integer
    FirstName                 String?  @db.Text() //
    MiddleName                String?  @db.Text() // Sometimes represented by just a middle initial.
    LastName                  String?  @db.Text() //  
    Gender                    String?  @db.Text() //"M", "F" or blank.

    // Demographics
    Business_Owner                         String?   @db.Text() // ConsumerData_Business_Owner
    CalculatedRegDate                      DateTime? @db.Date() // format 99/99/9999'; calculated reg date will be the same as official reg date except in those cases where the county has used it as a record update field in which case the calculated registration date will be set to 30 days prior to the oldest recorded vote date as a better indicator of the true registration date of the voter'
    CountyEthnic_Description               String?   @db.Text() // CountyEthnic_Description
    CountyEthnic_LALEthnicCode             String?   @db.Text() // CountyEthnic_LALEthnicCode
    CountyVoterID                          String?   @db.Text() // If available
    Education_Of_Person                    String?   @db.Text() // ConsumerData_Education_Of_Person
    Estimated_Income_Amount                String?   @db.Text() // ConsumerData_Estimated_Income_Amount
    Estimated_Income_Amount_Int            Int?      @db.Integer // Numeric version for range queries and comparisons
    EthnicGroups_EthnicGroup1Desc          String?   @db.Text() // 
    Ethnic_Description                     String?   @db.Text() // one of approximately 100 ethnic categories covering about 90% of voters with the remaining voters classifed as unknown'.  See Ethnic tab.'
    Homeowner_Probability_Model            String?   @db.Text() // ConsumerData_Homeowner_Probability_Model: Home Owner, Probable Home Owner, Renter
    Language_Code                          String?   @db.Text() // ConsumerData_Language_Code
    Mailing_Addresses_AddressLine          String?   @db.Text() // 
    Mailing_Addresses_ApartmentNum         String?   @db.Text() // 
    Mailing_Addresses_ApartmentType        String?   @db.Text() // 
    Mailing_Addresses_CassErrStatCode      String?   @db.Text() // Mailing_Addresses_CassErrStatCode
    Mailing_Addresses_CheckDigit           Int?      @db.Integer // Mailing_Addresses_CheckDigit
    Mailing_Addresses_City                 String?   @db.Text() // 
    Mailing_Addresses_Designator           String?   @db.Text() // 
    Mailing_Addresses_DPBC                 String?   @db.Text() // Mailing_Addresses_DPBC
    Mailing_Addresses_ExtraAddressLine     String?   @db.Text() // 
    Mailing_Addresses_HouseNumber          String?   @db.Text() // 
    Mailing_Addresses_PrefixDirection      Int?      @db.Integer // 
    Mailing_Addresses_State                String?   @db.Text() // 
    Mailing_Addresses_StreetName           String?   @db.Text() // 
    Mailing_Addresses_SuffixDirection      Int?      @db.Integer // 
    Mailing_Addresses_Zip                  String?   @db.Text() // 
    Mailing_Addresses_ZipPlus4             String?   @db.Text() // 
    Mailing_Families_FamilyID              String?   @db.Text() // Unique alpha-numeric ID starting with an M' for 'Mailing' that is common to all members of a mailing household address but different from all other mailing households.'
    Mailing_HHGender_Description           String?   @db.Text() // Cannot Determine,Female Only Household,Male Only Household,Mixed Gender Household
    Marital_Status                         String?   @db.Text() // ConsumerData_Marital_Status: Inferred Married, Inferred Single, Married, Single, Unknown
    MovedFrom_Date                         DateTime? @db.Date() // Voters_MovedFrom_Date
    MovedFrom_Party_Description            String?   @db.Text() // Voters_MovedFrom_Party
    MovedFrom_State                        String?   @db.Text() // Voters_MovedFrom_State
    NameSuffix                             String?   @db.Text() // Jr., Sr, etc.
    OfficialRegDate                        String?   @db.Text() // As reported by election authority.
    Parties_Description                    String?   @db.Text() // Full name of official party. See Parties tab.
    PlaceOfBirth                           String?   @db.Text() // Unformatted and non-standardized text field available only in some states.
    Presence_Of_Children                   String?   @db.Text() // ConsumerData_Presence_Of_Children_in_HH
    Residence_Addresses_AddressLine        String?   @db.Text() // 
    Residence_Addresses_ApartmentNum       String?   @db.Text() // 
    Residence_Addresses_ApartmentType      String?   @db.Text() // 
    Residence_Addresses_CassErrStatCode    String?   @db.Text() // Residence_Addresses_CassErrStatCode
    Residence_Addresses_CheckDigit         Int?      @db.Integer // Residence_Addresses_CheckDigit
    Residence_Addresses_City               String?   @db.Text() // 
    Residence_Addresses_Designator         String?   @db.Text() // 
    Residence_Addresses_DPBC               String?   @db.Text() // Residence_Addresses_DPBC
    Residence_Addresses_ExtraAddressLine   String?   @db.Text() // 
    Residence_Addresses_HouseNumber        String?   @db.Text() // 
    Residence_Addresses_LatLongAccuracy    String?   @db.Text() // Residence_Addresses_LatLongAccuracy
    Residence_Addresses_Latitude           String?   @db.Text() // 
    Residence_Addresses_Longitude          String?   @db.Text() //
    Residence_Addresses_PrefixDirection    Int?      @db.Integer // 
    Residence_Addresses_State              String?   @db.Text() // 
    Residence_Addresses_StreetName         String?   @db.Text() // 
    Residence_Addresses_SuffixDirection    Int?      @db.Integer // 
    Residence_Addresses_Zip                String?   @db.Text() // 
    Residence_Addresses_ZipPlus4           String?   @db.Text() // 
    Residence_HHParties_Description        String?   @db.Text() // Same as Mailing HH Parties calculation but based on those sharing the same residence address rather than those sharing the same mailing address
    SequenceOddEven                        String?   @db.Text() // Voters_SequenceOddEven
    SequenceZigZag                         String?   @db.Text() // Voters_SequenceZigZag
    StateVoterID                           String?   @db.Text() // State-supplied voter ID
    Veteran_Status                         String?   @db.Text() // ConsumerDataLL_Veteran
    VoterParties_Change_Changed_Party      String?   @db.Text() // Voter changed party affiliation between Dem and Rep
    Voter_Status                           String?   @db.Text() // In-house computed classification. One of: Unlikely, First Time, Likely, Super, Unknown
    Voter_Status_UpdatedAt                 DateTime?
    VoterTelephones_CellConfidenceCode     Int?      @db.Integer // 
    VoterTelephones_CellPhoneFormatted     String?   @db.Text() // 
    VoterTelephones_LandlineConfidenceCode Int?      @db.Integer // 
    VoterTelephones_LandlineFormatted      String?   @db.Text() // 

    // Voter Turnout
    AnyElection_2017                           String? @db.Text() // AnyElection_2017
    AnyElection_2019                           String? @db.Text() // AnyElection_2019
    AnyElection_2021                           String? @db.Text() // AnyElection_2021
    AnyElection_2023                           String? @db.Text() // AnyElection_2023
    AnyElection_2025                           String? @db.Text() // AnyElection_2025
    General_2016                               String? @db.Text() // General_2016
    General_2018                               String? @db.Text() // General_2018
    General_2020                               String? @db.Text() // General_2020
    General_2022                               String? @db.Text() // General_2022
    General_2024                               String? @db.Text() // General_2024
    General_2026                               String? @db.Text() // General_2026
    OtherElection_2016                         String? @db.Text() // OtherElection_2016
    OtherElection_2018                         String? @db.Text() // OtherElection_2018
    OtherElection_2020                         String? @db.Text() // OtherElection_2020
    OtherElection_2022                         String? @db.Text() // OtherElection_2022
    OtherElection_2024                         String? @db.Text() // OtherElection_2024
    OtherElection_2026                         String? @db.Text() // OtherElection_2026
    PresidentialPrimary_2016                   String? @db.Text() // PresidentialPrimary_2016
    PresidentialPrimary_2020                   String? @db.Text() // PresidentialPrimary_2020
    PresidentialPrimary_2024                   String? @db.Text() // PresidentialPrimary_2024
    Primary_2016                               String? @db.Text() // Primary_2016
    Primary_2018                               String? @db.Text() // Primary_2018
    Primary_2020                               String? @db.Text() // Primary_2020
    Primary_2022                               String? @db.Text() // Primary_2022
    Primary_2024                               String? @db.Text() // Primary_2024
    Primary_2026                               String? @db.Text() // Primary_2026
    VotingPerformanceEvenYearGeneral           String? @db.Text() // Voters_VotingPerformanceEvenYearGeneral
    VotingPerformanceEvenYearGeneralAndPrimary String? @db.Text() // Voters_VotingPerformanceEvenYearGeneralAndPrimary
    VotingPerformanceEvenYearPrimary           String? @db.Text() // Voters_VotingPerformanceEvenYearPrimary
    VotingPerformanceMinorElection             String? @db.Text() // Voters_VotingPerformanceMinorElection

    DistrictLinks DistrictVoter[]

    // Demographic Indexes
    @@index([FirstName])
    @@index([MiddleName])
    @@index([LastName])
    @@index([Active])
    @@index([Age])
    @@index([Age_Int])
    @@index([Gender])
    @@index([Parties_Description])
    @@index([VoterParties_Change_Changed_Party])
    @@index([CalculatedRegDate])
    @@index([OfficialRegDate])
    @@index([VoterTelephones_CellConfidenceCode])
    @@index([VoterTelephones_LandlineConfidenceCode])
    @@index([VoterTelephones_CellPhoneFormatted])
    @@index([VoterTelephones_LandlineFormatted])
    @@index([Mailing_Families_FamilyID])
    @@index([State, updatedAt])
    @@index([Business_Owner])
    @@index([Education_Of_Person])
    @@index([Estimated_Income_Amount])
    @@index([Estimated_Income_Amount_Int])
    @@index([EthnicGroups_EthnicGroup1Desc])
    @@index([Homeowner_Probability_Model])
    @@index([Language_Code])
    @@index([Marital_Status])
    @@index([Presence_Of_Children])
    @@index([Veteran_Status])
    @@index([Voter_Status])
    @@index([Voter_Status_UpdatedAt])
    // Voter Turnout Indexes
    @@index([VotingPerformanceEvenYearGeneral])
    @@index([VotingPerformanceEvenYearPrimary])
    @@index([VotingPerformanceEvenYearGeneralAndPrimary])
    @@index([VotingPerformanceMinorElection])
    @@schema("green")
}
